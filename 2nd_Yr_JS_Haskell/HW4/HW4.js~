function Ghost(color, x, y)
{
  while (x > 16)
  {
    x = (x%16) - 1
  }
  while (y > 16)
  {
    y = (y%16) - 1
  }
  var ghostColor = color;
  var cordX = x;
  var cordY = y;
}

Ghost.prototype.up = function () {
  cordY++;
};
Ghost.prototype.down = function () {
  cordY--;
};
Ghost.prototype.left = function () {
  cordX--;
};
Ghost.prototype.right = function () {
  cordY++;
};
Ghost.prototype.toString = funtion (foo) {
  return ("" + foo + "");
}
